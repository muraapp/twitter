<div class="container">
  <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
    <h3>ツイート確認画面</h3>
    <%= form_for(@tweet) do |f| %>
            <%= f.hidden_field :content %>
            <p><%= @tweet.content %></p>
            <%= f.submit "ツイート", class: 'btn btn-primary btn-block' %>
    <% end %>
    
    <%= form_for(@tweet, url: tweets_path, method: 'get') do |f| %>
        <%= f.hidden_field :content %>
        <%= f.submit "戻る", name: 'back',class: 'btn btn-default btn-block' %>
    <% end %>
    </div>
</div>